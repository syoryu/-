@-webkit-keyframes fadeIn {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
@keyframes fadeIn {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}
/* 旋转平台 */
.stage {
  width: 750rpx;
  height: 100vh;
  left: 0;
  position: absolute;
  bottom: 20vh;
  -webkit-transform-origin: 50% 150%;
  transform-origin: 50% 150%;
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
  transition: -webkit-transform 500ms cubic-bezier(0.455, 0.03, 0.515, 0.955);
  transition: transform 500ms cubic-bezier(0.455, 0.03, 0.515, 0.955);
  transition: transform 500ms cubic-bezier(0.455, 0.03, 0.515, 0.955), -webkit-transform 500ms cubic-bezier(0.455, 0.03, 0.515, 0.955);
  -webkit-animation: fadeIn 3s;
  animation: fadeIn 3s;
  /* 渐显动画过渡 */
}
.stage .page-item {
  z-index: 100;
  bottom: 0;
  left: 0;
  height: 100%;
  padding: 50vh 5vh 50vh 5vh;
  position: absolute;
  text-align: center;
  -webkit-transform-origin: 50% 150%;
  transform-origin: 50% 150%;
  width: 100%;
  box-sizing: border-box;
}
.stage .page-item:nth-child(2) {
  -webkit-transform: rotateZ(60deg);
  transform: rotateZ(60deg);
}
.stage .page-item:nth-child(3) {
  -webkit-transform: rotateZ(120deg);
  transform: rotateZ(120deg);
}
.stage .page-item:nth-child(4) {
  -webkit-transform: rotateZ(180deg);
  transform: rotateZ(180deg);
}
.stage .page-item:nth-child(5) {
  -webkit-transform: rotateZ(240deg);
  transform: rotateZ(240deg);
}
.stage .page-item .img-wrap {
  position: relative;
  width: 100%;
}
.stage .page-item .img-wrap .border {
  border-radius: 10px;
}
@keyframes fadeIn {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
}
}

