.page .ornament-bottom-wrap .ornament-bottom {
  width: 50vw;
  height: auto;
  -webkit-animation: fadeIn 1s;
  animation: fadeIn 1s;
  -webkit-filter: drop-shadow(0px -5px 6px #00000077);
          filter: drop-shadow(0px -5px 6px #00000077);
