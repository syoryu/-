.page .bg-img {
  position: absolute;
  left: 0;
  bottom: 0;
  display: block;
  width: 100%;
  height: 100vh;
  z-index: -990;
}
.page .confetti {
  position: absolute;
  left: 50%;
  top: 30vh;
  transform: translateX(-50%);
  -webkit-transform: translateX(-50%);
}
.page .usrAvatar {
  position: absolute;
  border-radius: 12px;
  left: 50%;
  top: 30vh;
  z-index: -1;
  margin-top: 0.5px;
  transform: translateX(-50%);
  -webkit-transform: translateX(-50%);
  background-color: rgba(255, 255, 255, 0.9);
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  box-shadow: 0 0 15px 5px rgba(0, 0, 0, 0.3);
  -webkit-animation: fadeIn 3s;
  animation: fadeIn 3s;
}
.page .Prev_next {
  z-index: 50;
  position: absolute;
  left: 50%;
  top: 30vh;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
}
.page .Prev_next .btn {
  height: auto;
}
.page .border-bar {
  z-index: 15;
  position: absolute;
  width: 100%;
  bottom: 37vh;
  left: 50%;
  transform: translateX(-50%);
  -webkit-transform: translateX(-50%);
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  -webkit-animation: fadeIn 3s;
  animation: fadeIn 3s;
  justify-content: center;
  background-color: rgba(136, 23, 7, 0.6);
}
.page .border-bar .border-item {
  display: flex;
  align-items: center;
  justify-content: center;
}
.page .border-bar .border-item .border-img {
  border-radius: 10px;
  width: 80%;
  height: 80%;
}
.page .border-bar .border-item .border-img-active {
  border-radius: 10px;
  width: 83%;
  height: 83%;
  box-shadow: 0 0 4px 3px rgba(255, 255, 255, 0.8);
}
.page .btn-wrap {
  z-index: 14;
  position: absolute;
  right: 3vh;
  bottom: 12vh;
  -webkit-animation: fadeIn 3s;
  animation: fadeIn 3s;
}
.page .btn-wrap button {
  border-radius: 10px;
  background-color: #0F3457;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  box-shadow: 5px 5px 10px rgba(34, 35, 79, 0.8);
}
.page .btn-wrap button:nth-child(1) {
  margin-bottom: 3vh;
  background-image: url(data:image/png;base64,);
}
.page .btn-wrap button:nth-child(2) {
  margin-bottom: 3vh;
  background-image: url(data:image/png;base64,);
}
.page .btn-wrap button:nth-child(3) {
  background-image: url(data:image/png;base64,);
}
.page .btn-wrap .btn-hover {
  transform: translate(3px, 3px);
  -webkit-transform: translate(3px, 3px);
  box-shadow: 0px 0px 8px rgba(34, 35, 79, 0.8);
}
.page .cropper-button {
  z-index: 20;
  position: fixed;
  width: 100%;
  bottom: 2em;
  display: flex;
  justify-content: space-evenly;
}
.page .cropper-button .button {
  width: 35%;
  max-width: 15em;
}
.page .cropper-button .button button {
  width: 100%;
  background-color: #0F3457;
  color: #DDB55F;
  font-weight: bolder;
  font-size: 100%;
  font-family: '华文楷体';
}
.page .mascot-wrap {
  z-index: 11;
  position: absolute;
  width: 70vw;
  height: auto;
  bottom: 4vh;
  left: 0;
  margin-left: -3%;
  -webkit-animation: bounce-down 5s linear infinite, fadeIn 3s;
  animation: bounce-down 5s linear infinite, fadeIn 3s;
}
.page .mascot-wrap .mascot {
  width: 100%;
  height: auto;
  -webkit-filter: drop-shadow(0px 2px 5px #ecf6ff);
          filter: drop-shadow(0px 2px 5px #ecf6ff);
}
@-webkit-keyframes bounce-down {
25% {
    -webkit-transform: translateY(-7px);
}
50%,
  100% {
    -webkit-transform: translateY(0);
}
75% {
    -webkit-transform: translateY(7px);
}
}
@keyframes bounce-down {
25% {
    -webkit-transform: translateY(-7px);
            transform: translateY(-7px);
}
50%,
  100% {
    -webkit-transform: translateY(0);
            transform: translateY(0);
}
75% {
    -webkit-transform: translateY(7px);
            transform: translateY(7px);
}
}
.page .blessing {
  z-index: 11;
  position: absolute;
  transition: all 0.3s;
  -webkit-animation: move 1s linear;
          animation: move 1s linear;
  -webkit-animation-fill-mode: forwards;
          animation-fill-mode: forwards;
  width: 96px;
  margin-left: -48px;
  height: auto;
}
@-webkit-keyframes move {
0% {
    margin-top: 0px;
    opacity: 1;
}
50% {
    margin-top: -50px;
    opacity: 0.5;
}
100% {
    margin-top: -100px;
    opacity: 0;
}
}
@keyframes move {
0% {
    margin-top: 0px;
    opacity: 1;
}
50% {
    margin-top: -50px;
    opacity: 0.5;
}
100% {
    margin-top: -100px;
    opacity: 0;
}
}
